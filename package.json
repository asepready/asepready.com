{
  "name": "bootstrapui",
  "version": "1.0.0",
  "description": "Bootstrap 5 HTML template created by Bootstrap",
  "scripts": {
    "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
    "build:assets": "node scripts/build-assets.js",
    "build:pug": "node scripts/build-pug.js",
    "build:scripts": "node scripts/build-scripts.js",
    "build:scss": "node scripts/build-scss.js",
    "clean": "node scripts/clean.js",
    "start": "npm run build && node scripts/start.js",
    "start:debug": "npm run build && node scripts/start-debug.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitsava/BootstrapUI.git"
  },
  "keywords": [
    "css",
    "sass",
    "html",
    "responsive",
    "theme",
    "template"
  ],
  "author": "Asep Ready",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gitsava/BootstrapUI/issues",
    "email": "asepdevops@gmail.com"
  },
  "homepage": "https://github.com/gitsava/BootstrapUI#readme",
  "dependencies": {
    "bootstrap": "5.1.3"
  },
  "devDependencies": {
    "autoprefixer": "10.3.7",
    "browser-sync": "2.27.5",
    "chokidar": "3.5.2",
    "concurrently": "6.3.0",
    "postcss": "8.3.9",
    "prettier": "2.4.1",
    "pug": "3.0.2",
    "sass": "1.43.2",
    "shelljs": "0.8.4",
    "upath": "2.0.1"
  }
}
